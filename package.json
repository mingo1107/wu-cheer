{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "watch": "vite build --watch",
        "watch:force": "git add . && vite build --watch",
        "dev:force": "git add . && vite",
        "build:force": "git add . && vite build",
        "force-update": "./scripts/force-update.sh",
        "watch:clean": "./scripts/force-update.sh && vite build --watch",
        "dev:clean": "./scripts/force-update.sh && vite"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.0",
        "postcss": "^8.4.0",
        
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^1.0.0",
        "tailwindcss": "^3.4.0",
        "vite": "^4.5.0"
    },
    "dependencies": {
        "@vitejs/plugin-vue": "^4.6.0",
        "vue": "^3.5.21",
        "vue-router": "^4.5.1"
    }
}
